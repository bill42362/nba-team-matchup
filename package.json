{
  "name": "nba-team-matchup",
  "version": "1.0.0",
  "description": "List next week matchup count for every team.",
  "main": "dist/index.js",
  "scripts": {
    "rmdist": "rimraf dist",
    "buildjs": "babel src -d dist --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "buildcss": "lessc src/client/css/index.less dist/client/css/index.css",
    "build": "npm run rmdist && npm run buildjs && npm run buildcss",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server",
    "devserver": "babel-node src/server",
    "devserver:auto": "nodemon --ignore dist --exec babel-node src/server",
    "wds": "webpack-dev-server --progress",
    "test": "jest"
  },
  "keywords": [
    "nba",
    "team",
    "matchup"
  ],
  "author": "Pi-Yin Hsiao",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.0.0-beta.2",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "nodemon": "^1.12.1",
    "normalize.less": "^1.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
